{
    "name": "gai871013/bzh-license",
    "type": "library",
    "description": "生成&查询授权",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "王朝",
            "email": "wang.gaichao@163.com"
        }
    ],
    "require": {
        "php": ">=7.0 || >=8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.2",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Gai871013\\License\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gai871013\\License\\ServiceProvider"
            ],
            "aliases": {
                "License": "Gai871013\\License\\Facades\\License"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
